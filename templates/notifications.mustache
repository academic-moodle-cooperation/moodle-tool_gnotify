<div id="gnotify-wrapper-sticky" class="row notification-sticky">
    {{#sticky}}
        <div id="{{id}}-gnotify-wrapper" class="gnotify gnotify-plr {{#padding}}gnotify-pt{{/padding}}">
            <div id="{{id}}-gnotify" class="card {{ntype}}">
                <div class="gnotify-notification-html card-body">
                    {{#dismissable}}
                        <button type="button" onclick="$.fn.tool_gnotify_acknowledge_notification({{id}});" class="close">&times;</button>{{/dismissable}}
                    {{{html}}}
                </div>
            </div>
        </div>
    {{/sticky}}
</div>
<div id="gnotify-wrapper-non-sticky" class="row notification-relative">
    {{#non-sticky}}
        <div id="{{id}}-gnotify-wrapper" class="gnotify gnotify-plr {{#padding}}gnotify-pt{{/padding}}">
            <div id="{{id}}-gnotify" class="card {{ntype}}">
                <div class="gnotify-notification-html card-body">
                    {{#dismissable}}
                        <button type="button" onclick="$.fn.tool_gnotify_acknowledge_notification({{id}});" class="close">&times;</button>{{/dismissable}}
                    {{{html}}}
                </div>
            </div>

        </div>
    {{/non-sticky}}
</div>