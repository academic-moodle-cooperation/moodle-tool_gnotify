{{#sticky.length}}
    <div id="gnotify-wrapper-sticky" class="row notification-sticky">
        {{#sticky}}
            <div id="{{id}}-gnotify-wrapper" class="gnotify gnotify-pt">
                <div id="{{id}}-gnotify" class="card">
                    <div class="gnotify-notification-html card-body">
                        <button type="button" onclick="$.fn.tool_gnotify_acknowledge_notification({{id}});" class="close" data-dismiss="alert">&times;</button>
                        {{{html}}}
                    </div>
                    <!-- button class="gnotify-close btn btn-secondary" onclick="$.fn.tool_gnotify_acknowledge_notification({{id}});">&times;</button-->
                </div>
            </div>
        {{/sticky}}
    </div>
{{/sticky.length}}
{{#non-sticky.length}}
    <div id="gnotify-wrapper-non-sticky" class="row notification-relative">
        {{#non-sticky}}
            <div id="{{id}}-gnotify-wrapper" class="gnotify gnotify-pt">
                <div id="{{id}}-gnotify" class="card">
                    <div class="gnotify-notification-html card-body">
                        <button type="button" onclick="$.fn.tool_gnotify_acknowledge_notification({{id}});" class="close" data-dismiss="alert">&times;</button>
                        {{{html}}}
                    </div>
                    <!-- button class="gnotify-close btn btn-secondary" onclick="$.fn.tool_gnotify_acknowledge_notification({{id}});">&times;</button-->
                </div>

            </div>
        {{/non-sticky}}
    </div>
{{/non-sticky.length}}