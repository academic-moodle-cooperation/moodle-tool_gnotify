 <tr>
 <th class="header name">{{# str }} templatetableheadername, tool_gnotify {{/ str }}</th>
 <th class="header actions">{{# str }} templatetableheaderactions, tool_gnotify {{/ str }}</th>
 </tr>
</thead>
<tbody>
{{#templates}}
 <tr>
   <td>
     {{name}}
   </td>
   <td>
   <a href="{{wwwroot}}/admin/tool/gnotify/usetemplate.php?templateid={{id}}"> {{# str }} usetemplate, tool_gnotify {{/ str }}</a>,
   <a href="{{wwwroot}}/admin/tool/gnotify/edit_template.php?templateid={{id}}"> {{# str }} edittemplate, tool_gnotify {{/ str }}</a>,
   <a href="{{wwwroot}}/admin/tool/gnotify/templates.php?action=delete&templateid={{id}}"> {{# str }} deletetemplate, tool_gnotify {{/ str }}</a>
   </td>
 </tr>
{{/templates}}
</tbody>
</table>
<a href="{{wwwroot}}/admin/tool/gnotify/create_template.php"><button class="btn btn-primary"/> {{# str }} createtemplate, tool_gnotify {{/ str }}</button></a>
