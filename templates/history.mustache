  {{!
    @template tool_gnotify/history

    Moodle history template.

    Example context (json):
    {

    }
  }}
<div class="course-content">
    <table id="gnotifications-{{uniqid}}" class="generaltable fullwidth topics">
        <tbody class="table">
        {{#ack}}
            <tr class="table-row {{ntype}}">
                <td class="table-cell">
                    {{{ html }}}
                </td>
                <td class="table-cell" style="text-align: right;">
                    <ul class="topics">
                        {{#ackdate}}
                        <li id="gndetail-{{ uniqid }}" class="main clearfix">{{# str }} acknowledgedon, tool_gnotify {{/ str }} {{ackdate}}</li>
                        {{/ackdate}}
                        {{^ackdate}}
                        <li id="gndetail-{{ uniqid }}" class="main clearfix">{{# str }} visiblefromto, tool_gnotify, { "fromdate": {{#quote}} {{ fromdate }}  {{/quote}}, "todate": {{#quote}} {{ todate }} {{/quote}} }{{/ str }}</li>
                        {{/ackdate}}
                    </ul>
                </td>
            </tr>
        {{/ack}}
        </tbody>
    </table>
</div>